<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>Disposify</id>
        <version>0.1.0</version>
        <authors>ruccho</authors>
        <description>Disposify is a source generator to subscribe / unsubscribe C# events with using-IDisposable
            pattern.
        </description>
        <repository type="git" url="https://github.com/ruccho/Disposify"/>
        <dependencies>
            <group targetFramework=".NETStandard2.1"/>
        </dependencies>
        <readme>README.md</readme>
    </metadata>
    <files>
        <file target="\" src="..\README.md"/>
        <file target="lib\netstandard2.1\" src="bin\$Configuration$\netstandard2.1\Disposify.dll"/>
        <file target="analyzers\dotnet\cs\" src="bin\$Configuration$\netstandard2.1\Disposify.Generators.dll"/>
    </files>
</package>